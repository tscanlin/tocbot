{
  "parser": "babel-eslint",
  "rules": {
    "brace-style": 1,
    "camelcase": 1,
    "comma-dangle": 1,
    "curly": 1,
    "eol-last": 0,
    "eqeqeq": 1,
    "guard-for-in": 0,
    "new-cap": 0,
    "no-alert": 0,
    "no-bitwise": 0,
    "no-caller": 1,
    "no-cond-assign": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-empty": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extra-semi": 1,
    "no-invalid-regexp": 2,
    "no-mixed-spaces-and-tabs": 1,
    "no-new": 1,
    "no-obj-calls": 2,
    "no-shadow": 1,
    "no-sparse-arrays": 1,
    "no-trailing-spaces": 1,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 2,
    "no-unused-vars": 0,
    "quotes": [1, "single"],
    "semi": 0,
    "strict": [0, "global"],
    "valid-jsdoc": 0,
    "valid-typeof": 1,
    "wrap-iife": [2, "any"]
  },
  "globals": {
    "$": true,
    "console": true,
    "describe": true,
    "it": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "expect": true,
    "jQuery": true,
    "require": true,
    "xdescribe": true,
    "xit": true
  },
  "env": {
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  }
}
